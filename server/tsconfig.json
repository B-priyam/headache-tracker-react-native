{
  "compilerOptions": {
    "target": "es6",                           
    "module": "node16",                           
    "moduleResolution": "node16",                 
    "esModuleInterop": true,                      
    "allowSyntheticDefaultImports": true,         
    "allowImportingTsExtensions": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "noEmit": true,                     
    "forceConsistentCasingInFileNames": true      
  },
}